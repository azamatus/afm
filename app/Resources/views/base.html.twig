<html> <!--<![endif]-->
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Afm{% endblock %}</title>
    <meta name="Description" content="Интернет-Магазин Afm.{% block meta_description %}{% endblock %}">
    <meta property="og:image" content="{% block facebook_image_url %}{{ asset('public/img/logo.png') }}{% endblock %}"/>
    <!-- Mobile Specific Metas
  ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <link type="text/css" rel="stylesheet" href="{{ asset("public/css/bootstrap.min.css") }}" media="screen"/>
    <link type="text/css" rel="stylesheet" href="{{ asset("public/css/font-awesome.min.css") }}" media="screen"/>
    <link type="text/css" rel="stylesheet" href="{{ asset("public/css/prettyPhoto.css") }}" media="screen"/>
    <link type="text/css" rel="stylesheet" href="{{ asset("public/css/price-range.css") }}" media="screen"/>
    <link type="text/css" rel="stylesheet" href="{{ asset("public/css/animate.css") }}" media="screen"/>
    <link type="text/css" rel="stylesheet" href="{{ asset("public/css/main.css") }}" media="screen"/>
    <link type="text/css" rel="stylesheet" href="{{ asset("public/css/responsive.css") }}" media="screen"/>
    <link type="text/css" rel="stylesheet" href="{{ asset("public/css/jquery-ui-1.10.1.min.css") }}"/>
    {% block stylesheets %}{% endblock %}
    <script type="text/javascript" src="{{ asset("public/js/html5shiv.js") }}"></script>
    <script type="text/javascript">
        var base_url = "{{ path('nurix_homepage') }}";
    </script>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" sizes="16x16"/>
</head>
<body>
<div id="mainContainer" class="clearfix">
    {% include '::header.html.twig' %}
    <div id="tovar-added" title="{{ 'product_info.addedToCard'|trans({}, 'CatalogBundle') }}"></div>
    {% block slider %}
    {% endblock %}
    <div class="container">
    {% block body %}{% endblock %}
    </div>
    {% include '::footer.html.twig' %}
</div>
<script type="text/javascript" src="{{ asset("public/js/jquery.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/bootstrap.min.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/html5placeholder.jquery.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.scrollUp.min.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery-ui.min.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/price-range.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.prettyPhoto.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/main.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.validate.js") }}"></script>
<!--<script type="text/javascript" src="{{ asset("bundles/applicationsonatauser/js/registration.js") }}"></script>-->
<script type="text/javascript" src="{{ asset("bundles/catalog/js/bin.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/custom.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.cookie.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.cycle2.min.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.cycle2.carousel.min.js") }}  "></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.flexslider-min.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.tweet.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/jquery.fancybox.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/noty/jquery.noty.packaged.min.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/noty/center.js") }}"></script>
<script type="text/javascript" src="{{ asset("public/js/noty/default.js") }}"></script>
<script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
<script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
<script src="{{ asset('bundles/catalog/js/review.js') }}" type="text/javascript"></script>
{% block javascripts %}
{% endblock %}
</body>
</html>

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Nurix.kg{% endblock %}</title>
    {% block stylesheets %}
        <link type="text/css" rel="stylesheet" href="{{ asset('public/css/mainframe.css') }}"/>
        <link type="text/css" rel="stylesheet" href="{{ asset("public/css/facebook.css") }}"/>
        <link type="text/css" rel="stylesheet" href="{{ asset("bundles/applicationsonatauser/css/sonata.css") }}"/>
        <link type="text/css" rel="stylesheet" href="{{ asset("public/css/jquery-ui.css") }}"/>
    {% endblock %}
    <script type="text/javascript" language="javascript">
        var base_url = "{{ path('nurix_homepage') }}";
    </script>
    {% block javascripts %}
        <script type="text/javascript" src="{{ asset("public/js/facebook.js") }}"></script>
        <script type="text/javascript" src="{{ asset("public/js/jquery-1.9.0.min.js") }}"></script>
        <script type="text/javascript" src="{{ asset("public/js/html5placeholder.jquery.js") }}"></script>
        <script type="text/javascript" src="{{ asset("public/js/jquery-ui.min.js") }}"></script>
        <script type="text/javascript" src="{{ asset("public/js/jquery.validate.js") }}"></script>
        <script type="text/javascript" src="{{ asset("bundles/applicationsonatauser/js/registration.js") }}"></script>
        <script type="text/javascript" src="{{ asset("public/js/jquery.pwstrength.js") }}"></script>
        <script type="text/javascript" src="{{ asset("bundles/catalog/js/bin.js") }}"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        <script>
